<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="GoFetch" transaction-type="RESOURCE_LOCAL">
		<class>com.gofetch.entities.URL</class>
		<class>com.gofetch.entities.Keywords</class>
		<class>com.gofetch.entities.Link</class>
		<class>com.gofetch.entities.SEOMozData</class>
		<class>com.gofetch.entities.TwitterMention</class>
		<class>com.gofetch.entities.MiscSocialData</class>
		<properties>
			<property name="javax.persistence.jdbc.driver" value="com.google.appengine.api.rdbms.AppEngineDriver"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:google:rdbms://kastle2gofetch:url/url"/>
			<property name="javax.persistence.jdbc.user" value="kastle2"/>
			<property name="javax.persistence.jdbc.password" value="kastle2"/>
		</properties>
	</persistence-unit>
</persistence>
